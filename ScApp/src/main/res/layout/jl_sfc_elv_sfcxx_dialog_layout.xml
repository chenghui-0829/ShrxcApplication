                        android:textColor="@color/app_text_color_999999"
                        android:textSize="12dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_columnWeight="1"
                    android:layout_gravity="fill"
                    android:layout_marginTop="0.5dp"
                    android:layout_rowWeight="1"
                    android:background="@color/app_white_color_ffffff"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="26+"
                        android:textColor="@color/app_text_color_333333"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="3.25"
                        android:textColor="@color/app_text_color_999999"
                        android:textSize="12dp" />
                </LinearLayout>
            </GridLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@drawable/app_bottom_whilte_circle_color"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="取消"
                android:textColor="@color/app_text_color_666666"
                android:textSize="14dp" />

            <View
                android:layout_width="0.5dp"
                android:layout_height="35dp"
                android:background="@color/app_line_color_e6e6e6" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="确定"
                android:textColor="@color/app_text_color_333333"
                android:textSize="14dp" />
